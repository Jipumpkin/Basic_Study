package 학생;

public class Student {
	
	String name;
	String number;
	int age;
	int scoreJava;
	int scoreWeb;
	int scoreAndroid;
	
	public Student() {
		
	}
	public Student(String Na, String Num, int Ag, int scojava, int scoweb, int scoand) {
		name = Na;
		Num = number;
		Ag = age;
		scojava = scoreJava;
		scoweb = scoreWeb;
		scoand = scoreAndroid;
		
	}
	
	
	
	
	
	
	
	
}
