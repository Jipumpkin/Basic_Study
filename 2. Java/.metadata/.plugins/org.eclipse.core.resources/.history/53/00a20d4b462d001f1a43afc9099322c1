package 마우스;

public class Main {

	public static void main(String[] args) {
		//BallMouse
		BallMouse mouse1 = new BallMouse();
		
		mouse1.drag();
		mouse1.leftClick();
		mouse1.rightClick();
		
		WheelMouse mouse2 = new WheelMouse();
		
		mouse2.drag();
		mouse2.leftClick();
		mouse2.rightClick();
		
		
	}

}
